generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  id       Int    @id @default(autoincrement())
  User     User?
  password String
}

model User {
  id        Int     @id @default(autoincrement())
  username  String  @unique
  account   Account @relation(fields: [accountId], references: [id])
  accountId Int     @unique
}

model Feedback {
  id      Int    @id @default(autoincrement())
  summary String
  body    String
}
